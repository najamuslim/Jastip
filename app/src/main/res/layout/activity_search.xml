<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/signup"
    tools:context=".Search">


    <TextView
        android:id="@+id/textView4"
        android:layout_width="72dp"
        android:layout_height="31dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:text="Search"
        android:textColor="#ffffff"
        android:textSize="23sp"
        android:textStyle="bold" />

    <ImageButton
        android:id="@+id/backbtn1"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/backbtn" />

    <RelativeLayout
        android:layout_width="350dp"
        android:layout_height="445dp"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="80dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="115dp"
        android:background="@drawable/searchset"
        android:gravity="center">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="216dp"
            android:layout_height="31dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="58dp"
            android:layout_marginTop="28dp"
            android:layout_marginRight="58dp"
            android:text="Search For Travelers"
            android:textColor="#000"
            android:textSize="23sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="29dp"
            android:layout_height="14dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="28dp"
            android:layout_marginTop="90dp"
            android:text="City"
            android:textColor="#000"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="28dp"
            android:layout_height="19dp"
            android:layout_alignStart="@+id/textView8"
            android:layout_alignParentTop="true"
            android:layout_marginTop="131dp"
            android:text="Date"
            android:textColor="#000"
            android:textSize="14sp" />

        <EditText
            android:id="@+id/editText5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="71dp"
            android:layout_marginEnd="32dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text=""
            android:textColorHighlight="#39619E" />

        <EditText
            android:id="@+id/editText7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="115dp"
            android:layout_marginEnd="32dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:text=""
            android:textColorHighlight="#39619E" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="130dp"
            android:layout_marginEnd="52dp"
            android:background="@drawable/calenderbtn" />

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginStart="-48dp"
            android:layout_marginTop="135dp"
            android:layout_toEndOf="@+id/imageButton5"
            android:background="@drawable/downarrowsmall" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="400dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="180dp"
            android:layout_marginEnd="0dp"
            app:srcCompat="@drawable/path243" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="57dp"
            android:layout_marginEnd="60dp"
            android:layout_marginRight="56dp"
            android:layout_marginBottom="176dp"
            android:text="Show curent/recent traveler's posts"
            android:textColor="#000" />

        <ImageButton
            android:id="@+id/searchbtn1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="30dp"
            android:layout_marginLeft="22dp"
            android:layout_marginRight="22dp"
            android:layout_marginBottom="32.5dp"
            android:background="@drawable/searhbtn" />

    </RelativeLayout>
</RelativeLayout>